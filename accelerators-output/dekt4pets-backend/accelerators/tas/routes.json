{
  "routes": [
    {
      "title": "Retrieve pets",
      "description": "Retrieve a list of all available pets.",
      "path": "/api/pets",
      "method": "GET",
      "filters": [ "RateLimit=2,10s" ]
    },
    {
      "title": "Update adoptions",
      "description": "Update an adoptions",
      "path": "/api/pets/adoptions/",
      "method": "POST,PUT,DELETE",
      "token-relay": true,
      "sso-enabled": true
    }
  ]
}
